{
    "name": "terminusstudio/ezdb",
    "description": "ezDB is a lightweight library that provides an easy and fast to deal with databases in PHP. It manages connections, provides a query builder, and a lightweight ORM.\n",
    "type": "library",
    "keywords": ["orm","database","mysql","pdo", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sugavanas",
            "email": "sugavanas@terminus.studio",
            "homepage": "https://sugavanas.me"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "TS\\ezDB\\": "src",
            "TS\\ezDB\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-dev": "1.x-dev"
        }
    }
}
